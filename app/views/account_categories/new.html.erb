<h1>New account category</h1>

<% form_for(@ac_cate) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :label %><br />
    <%= f.text_field :label %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create new account' %>
  </p>
<% end %>

<%= link_to 'Previous', account_categories_path %>